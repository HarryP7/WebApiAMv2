<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.harry.Entity.ServiceEntity" table="service" schema="webapiam">
        <id name="idService">
            <column name="Id_Service" sql-type="int(10)"/>
        </id>
        <property name="lat">
            <column name="Lat" sql-type="decimal(10,6)" precision="10" scale="6"/>
        </property>
        <property name="lng">
            <column name="Lng" sql-type="decimal(10,6)" precision="10" scale="6"/>
        </property>
        <property name="title">
            <column name="Title" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="datePlace">
            <column name="DatePlace" sql-type="datetime(6)"/>
        </property>
        <property name="imageUrl">
            <column name="ImageUrl" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>