<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.harry.Entity.StatusEntity" table="status" schema="webapiam">
        <id name="idStatus">
            <column name="Id_Status" sql-type="int(10)"/>
        </id>
        <property name="sName">
            <column name="sName" sql-type="varchar(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>